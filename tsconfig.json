{
    "compilerOptions": {
        "outDir": "./build",
        "allowJs": true,
        "target": "es5",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "downlevelIteration": true
    },
    "include": [
        "./cli-utils/**/*"
    ]
}